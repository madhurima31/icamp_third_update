        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Activity_Landing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Student_Login" />
        <activity android:name=".Activity.Dashboard_Student" />
        <activity android:name=".Activity.Dashboard_Company" />
        <activity android:name=".Activity.Dashboard_Admin" />
        <activity android:name=".Activity.Student_Register" />
        <activity android:name=".Activity.Company_Register" />
